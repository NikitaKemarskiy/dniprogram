<section id="pricing">
	<div class="container">
		<div class="row header">
			<div class="col-lg-12 text-center">
				<h2 class="section-heading"><%= local.pricing.heading %></h2>
				<span class="separator"></span>
			</div>
		</div>
		<div class="row">
			<div class="col-md-4">
				<div class="row pricing-title"><%= local.pricing.content1.heading %></div>
				<div class="row pricing">
					<div class="col-lg-3 col-md-3 col-sm-3" >
						<div class="row">
							<span class="pricing-price"><span class="currency">$</span>150+</span>
						</div>
					</div>
					<div class="pricing-row"><span class="pricing-value">
						<span class="pricing-option">
							<i class="fa fa-check"></i>
							<%= local.pricing.chatbot.content1 %>
						</span>
					</span></div>
					<div class="pricing-row"><span class="pricing-value">
						<span class="pricing-option">
							<i class="fa fa-check"></i>
							<%= local.pricing.chatbot.content2 %>
						</span>
					</span></div>
					<div class="pricing-row"><span class="pricing-value">
						<span class="pricing-option">
							<i class="fa fa-times"></i>
							<%= local.pricing.chatbot.content3 %>
						</span>
					</span></div>
					<div class="pricing-row button-container">
						<a class="secondary-button secondary-button-inverse" onclick="openForm()">Выбрать</a>
					</div>
				</div>
			</div>
			<div class="col-md-4">
