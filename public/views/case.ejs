		<!-- CASE -->
		<section id="case">
			<div class="container">
				<div class="row header">
					<div class="col-lg-12 text-center">
						<h2 class="section-heading"><%= local.cases.case.desc.heading %></h2>
						<span class="separator"></span>
					</div>
				</div>
				<div class="row desc margin-bottom-30">
					<div class="col-xs-12 col-sm-6">
						<h4>üìå <%= local.cases.case.desc.case %></h4>
						<p><%- local.cases.services.chatbot[caseName].case.desc.case %></p>
					</div>
					<div class="col-xs-12 col-sm-6">
						<h4>üë®‚Äç‚öñÔ∏è <%= local.cases.case.desc.customer %></h4>
						<p><%- local.cases.services.chatbot[caseName].case.desc.customer %></p>
					</div>
					<div class="col-xs-12 col-sm-6">
						<% if (local.cases.services.chatbot[caseName].case.desc.levels) { %>
							<h4>üìù <%= local.cases.case.desc.levels %></h4>
							<p>
								<ul>
									<% for (let i = 0; i < local.cases.services.chatbot[caseName].case.desc.levels.length; i++) { %>
										<li><%= local.cases.services.chatbot[caseName].case.desc.levels[i] %></li>
									<% } %>
								</ul>
							</p>
						<% } else { %>
							<h4>üìù <%= local.cases.case.desc.functional %></h4>
							<p>
								<ul>
									<% for (let i = 0; i < local.cases.services.chatbot[caseName].case.desc.functional.length; i++) { %>
										<li><%= local.cases.services.chatbot[caseName].case.desc.functional[i] %></li>
									<% } %>
								</ul>
							</p>
						<% } %>
					</div>
					<div class="col-xs-12 col-sm-6">
						<h4>üíØ <%= local.cases.case.desc.result %></h4>
						<p>
							<%- local.cases.services.chatbot[caseName].case.desc.result %>
							<!-- <%= local.cases.case.desc.working %>: <a href="https://<%= local.cases.services.chatbot[caseName].case.desc.result %>"><%= local.cases.services.chatbot[caseName].case.desc.result %></a>
							<% if (local.cases.services.chatbot[caseName].case.desc.isLimited) { %> <br><%= local.cases.services.chatbot[caseName].case.desc.limitedText %> <% } %> -->
						</p>
					</div>
					<% if (local.cases.services.chatbot[caseName].case.desc.desc) { %>
						<div class="col-xs-12">
							<h4>üí° <%= local.cases.case.desc.desc %></h4>
							<p><%= local.cases.services.chatbot[caseName].case.desc.desc %></p>
						</div>
					<% } %>
				</div>
				<div class="row header">
					<div class="col-lg-12 text-center">
						<h2 class="section-heading"><%= local.cases.case.screens.heading %></h2>
						<span class="separator"></span>
					</div>
				</div>
				<% if (local.cases.services.chatbot[caseName].case.screens) { %>
					<div class="row screens">
						<% for (let i = 0; i < local.cases.services.chatbot[caseName].case.screens.length; i++) { %>
							<div class="col-xs-12 col-sm-6 col-md-4">
								<div class="heading"><%= local.cases.services.chatbot[caseName].case.screens[i].title %></div>
								<img src="/img/cases/chatbot/<%= caseName %>/<%= local.cases.services.chatbot[caseName].case.screens[i].fileName %>" style="width: 100%; <% if (local.cases.services.chatbot[caseName].case.isWindows) { %> margin: 10px 0 20px 0; <% } %>" alt="">
							</div>
						<% } %>
					</div>
				<% } %>
				<% if (local.cases.services.chatbot[caseName].case.screensFullScreen) { %>
					<div class="row screens">
						<% for (let i = 0; i < local.cases.services.chatbot[caseName].case.screensFullScreen.length; i++) { %>
							<div class="col-12">
								<div class="heading"><%= local.cases.services.chatbot[caseName].case.screensFullScreen[i].title %></div>
								<img src="/img/cases/chatbot/<%= caseName %>/<%= local.cases.services.chatbot[caseName].case.screensFullScreen[i].fileName %>" style="width: 100%; <% if (local.cases.services.chatbot[caseName].case.isWindows) { %> margin: 10px 0 20px 0; <% } %>" alt="">
							</div>
						<% } %>
					</div>
				<% } %>
			</div>
		</section>
		<!-- END CASE -->
		<!-- CONTACT -->
		<section class="footer-contact-area section_padding_100 clearfix" id="contact">
			<div class="container">
				<div class="row">
					<div class="col-md-6">
						<!-- Heading Text  -->
						<div class="section-heading">
							<h2><%= local.contact.heading %></h2>
							<div class="line-shape"></div>
						</div>
						<div class="footer-text">
							<p><%- local.contact.text %></p>
						</div>
						<div class="address-text">
							<p><span><i class="fa fa-paper-plane" aria-hidden="true"></i>Telegram:</span><a href="https://t.me/kemarskiy"> @kemarskiy</a></p>
						</div>
						<div class="phone-text">
							<p><span><i class="fa fa-phone" aria-hidden="true"></i><%= local.contact.phone1 %></span><a href="tel:+380963705888"> +380-96-370-58-88</a></p>
							<p><span><i class="fa fa-phone" aria-hidden="true"></i><%= local.contact.phone2 %></span><a href="tel:+380977351159"> +380-97-735-11-59</a></p>
						</div>
						<div class="email-text">
							<p><span><i class="fa fa-envelope"></i>Email:</span><a href="mailto:dniprogramua@gmail.com"> dniprogramua@gmail.com</a></p>
						</div>
					</div>
					<div class="col-md-6">
						<!-- CONTACT FORM -->
						<form
							action="/<%= contentLanguage %>/mail"
							method="post"
							name="sentMessage"
							id="contactForm"
						>
							<div class="row">
								<div class="form-group">
									<input type="text" class="form-control" placeholder="<%= local.contact.form.input.name %>" id="name" name="name" required>
									<p class="help-block text-danger"></p>
								</div>
								<div class="form-group pull-right">
									<input type="email" class="form-control" placeholder="Email" id="email" name="email">
									<p class="help-block text-danger"></p>
								</div>
								<div class="form-group full-size">
									<input type="tel" class="form-control" placeholder="<%= local.contact.form.input.phone %>" id="phone" name="phone">
									<p class="help-block text-danger"></p>
								</div>
								<div class="form-group form-textarea">
									<textarea class="form-control" placeholder="<%= local.contact.form.input.message %>" id="message" name="message"></textarea>
									<p class="help-block text-danger"></p>
								</div>
								<input name="service" type="text" value="<%= local[service].service %>" class="display-none">
								<div class="clearfix"></div>
								<div class="text-center">
									<div id="success"></div>
									<button type="submit" class="btn btn-xl"><%= local.contact.form.input.send %></button>
								</div>
							</div>
						</form>
						<!-- END CONTACT FORM -->
					</div>
				</div>
			</div>
		</section>
		<!-- END CONTACT -->
